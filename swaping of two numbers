#include<stdio.h>
int main()
{
    int x,y,*a,*b,temp;
    
    printf("enter the value of x and y ");
    scanf("%d%d",&x,&y);
    
    printf("Before shopping \nx = %d\ny = %d\n",x,y);
    
    a=&x;
    b=&y;
    
    temp=*b;
    *b=*a;
    *a=temp;
    
    printf("after swapping\nx = %d\ny = %d\n",x,y);
    
    return 0;
    
    
}
